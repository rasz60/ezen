
import java.io.*;

public class BufferedOutputStreamEx01 {

	public static void main(String[] args) {
		try {
			FileOutputStream fos = new FileOutputStream("123.txt");
			BufferedOutputStream bos = new BufferedOutputStream(fos, 5);
			
			for( int i = '1'; i <= '99'; i++ ) {
				bos.write(i);
			}
			
			bos.close();
		} catch (IOException ie) {
			ie.printStackTrace();
		}
	}

}
